  "scripts": {
    "dev": "cross-env NODE_ENV=development tsx server/index.ts",
    "dev:client": "cd client && vite",
    "dev:server": "cross-env NODE_ENV=development tsx server/index.ts",
    "dev:both": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "dev:reset": "node scripts/dev-reset.js",
    "init": "tsx scripts/initialize.ts",
    "test:auth": "tsx scripts/test-auth.ts",
    "test:system": "tsx scripts/system-test.ts",
    "build": "node scripts/build.js",
    "build:client": "cd client && npm run build",
    "build:server": "npx esbuild server/index.ts --platform=node --packages=external --bundle --format=esm --outdir=dist",
    "start": "NODE_ENV=production node dist/index.js",
    "start:client": "cd client && vite preview",
    "check": "tsc",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "test": "cd tests && node run-all-tests.js",
    "test:payment": "cd tests && node payment-workflow-runner.js",
    "test:whatsapp": "cd tests && node payment-workflow.test.js",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "db:push": "drizzle-kit push",
    "verify-data-flow": "tsx scripts/verify-data-flow.ts"
  },
